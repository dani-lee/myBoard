<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="Member-Mapper">
	<select id="selectMemberList" resultType="member">
		select
		ID,
		PWD,
		EMAIL,
		PICTURE,
		ENABLED,
		REGDATE,
		PHONE,
		NAME,
		REGISTER,
		ADDRESS,
		AUTHORITY
		from
		member
	</select>	
 </mapper>